    "\n",
    "def fact(n):\n",
    "    \n",
    "    f = 1\n",
    "    \n",
    "    for i in range(1,n+1):\n",
    "        \n",
    "            f=f*i\n",
    "            \n",
    "    return f\n",
    "    \n",
    "x = int((input(\"Enter a number: \")))\n",
    "\n",
    "result=fact(x)\n",
    "\n",
    "\n",
    "print(result)"
   ]
